{
  "name": "visual-testing-storybook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "start-server-and-test": "^1.9.0"
  },
  "scripts": {
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:ci": "start-server-and-test storybook http://localhost:9009 test:coverage",
    "storybook": "start-storybook -p 9009 -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@storybook/addon-actions": "^5.0.8",
    "@storybook/addon-links": "^5.0.8",
    "@storybook/addon-storyshots": "^5.0.10",
    "@storybook/addon-storyshots-puppeteer": "^5.0.10",
    "@storybook/addons": "^5.0.8",
    "@storybook/react": "^5.0.8",
    "babel-loader": "^8.0.5",
    "jest-puppeteer": "^4.1.1",
    "puppeteer": "^1.15.0",
    "react-test-renderer": "^16.8.6"
  }
}
